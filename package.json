  "bugs": {
    "url": "https://github.com/vigour-io/hub-persistent/issues"
  },
  "homepage": "https://github.com/vigour-io/hub-persistent#readme",
  "dependencies": {
    "basho-riak-client": "^2.4.0",
    "brisky-stamp": "^3.0.0"
  },
  "devDependencies": {
    "tape": "^4.0.0",
    "ducktape": "^1.0.0",
    "tap-difflet": "0.7.0",
    "nodemon": "^1.0.0",
    "coveralls": "^2.0.0",
